@using RaccoonBlog.NancyFE.ViewModels
@inherits RaccoonBlog.NancyFE.RazorRssView<BlogPostsViewModel>
@foreach (var item in Model)
{
    <item>
        <title>@item.Title</title>
        <description>@item.Body</description>
        <pubdate>@item.PublishAt.ToString("r")</pubdate>
        <link>@Html.RenderContext.ParsePath("~/" + item.Path)</link>
    </item>
}